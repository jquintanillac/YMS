@model YMSweb.Data.Entities.MDAnexos

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<div class="container">
    <div class="card">
        <div class="card-header">
            <div class="div-title">
                <h4 class="m-0">CREAR ANEXOS</h4>
            </div>
        </div>
        <div class="card-body">
            <div class="row">
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="anex_desc" class="control-label"></label>
                        <input asp-for="anex_desc" class="form-control" />
                        <span asp-validation-for="anex_desc" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="anex_cod" class="control-label"></label>
                        <input asp-for="anex_cod" class="form-control" />
                        <span asp-validation-for="anex_cod" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label class="checkbox">Activa</label><br />
                        <input type="checkbox" asp-for="anex_act" value="false" >
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Crear" class='btn btn-info btn-sm text-white' />
                        <a class='btn btn-info btn-sm text-white' asp-action="Index">Regresar</a>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
