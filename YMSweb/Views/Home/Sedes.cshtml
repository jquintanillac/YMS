                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="card text-center">
                        <img class="card-img-top" src="/AdminLTE-master/img/puno.jpg" alt="Card image cap" height="240">
                        <div class="card-body">
                            <form asp-action="Principal" asp-controller="Home" method="post">
                                <input type="hidden" name="id_sede" id="id_sede" value="HUA" />
                                <input type="submit" class="btn btn-primary btn-sm" Value="PUERTO MALDONADO" />
                            </form>
                        </div>
                    </div>
                </div>
                @*<div class="col-lg-4">
                        <div class="card text-center">
                            <img class="card-img-top" src="/azzara/assets/img/moquegua.jpg" alt="Card image cap" height="240">
                            <div class="card-body">
                                <a href="#" class="card-link">Moquegua</a>
                            </div>
                        </div>
                    </div>*@
            </div>
            @*<div class="row">
                    <div class="col-lg-4">
                        <div class="card text-center">
                            <img class="card-img-top" src="/azzara/assets/img/puno.jpg" alt="Card image cap" height="240">
                            <div class="card-body">
                                <a href="#" class="card-link">Puno</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <div class="card text-center">
