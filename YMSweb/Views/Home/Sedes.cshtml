                                <input type="submit" class="btn btn-primary btn-sm" Value="AYACUCHO" />
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-4">
                    <div class="card text-center">
                        <img class="card-img-top" src="/AdminLTE-master/img/Chiclayo.jpg" alt="Card image cap" height="240">
                        <div class="card-body">
                            <form asp-action="Principal" asp-controller="Home" method="post">
                                <input type="hidden" name="id_sede" id="id_sede" value="CUS" />
